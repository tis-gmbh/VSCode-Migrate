{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2020",
        "lib": [
            "ES2020"
        ],
        "sourceMap": true,
        "rootDir": ".",
        "strict": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noUncheckedIndexedAccess": true,
        "noUnusedParameters": true,
        "skipLibCheck": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    }
}
